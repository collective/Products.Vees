<?xml version="1.0"?>
<object name="VeeSection" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Vee Section</property>
 <property name="description" i18n:translate=""></property>
 <property name="icon_expr"></property>
 <property name="factory">VeeSection</property>
 <property name="add_view_expr">string:${folder_url}/++add++VeeSection</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
 </property>
 <property name="allow_discussion">True</property>
 <property name="default_view">view</property>
 <property name="view_methods">
 </property>
 <property name="default_view_fallback">True</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">Products.Vees.content.Section</property>
 <property name="behaviors">
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
  <element
     value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
 </property>
 <property name="schema">Products.Vees.interfaces.ISection</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True" i18n:attributes="title">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    icon_expr="" link_target="" url_expr="string:${object_url}/edit"
    visible="True" i18n:attributes="title">
  <permission value="Modify portal content"/>
 </action>
</object>

